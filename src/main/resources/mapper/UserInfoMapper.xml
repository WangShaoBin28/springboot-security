                #{userName},
            </if>
            <if test="passWord != null">
                #{passWord},
            </if>
        </trim>
    </insert>
    <update id="updateByPrimaryKeySelective" parameterType="com.app.entity.UserInfoEntity">

        update user_info
        <set>
            <if test="userName != null">
                user_name = #{userName},
            </if>
